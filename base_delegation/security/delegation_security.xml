<odoo>
    <record id="delegation_rule" model="ir.rule">
        <field name="name">Users can act on their own delegations only</field>
        <field name="model_id" ref="model_delegation" />
        <field name="domain_force">[('delegator_id', '=', user.id)]</field>
        <field
            name="groups"
            eval="[
            (4, ref('base.group_user')),
        ]"
        />
        <field name="perm_read" eval="False" />
    </record>
</odoo>

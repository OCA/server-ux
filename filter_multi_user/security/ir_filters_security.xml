<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">
    <record id="ir_filters_employee_rule" model="ir.rule">
        <field name="name">ir.filters.multiuser</field>
        <field name="model_id" ref="base.model_ir_filters" />
        <field name="domain_force">['|',
            '|', ('user_id', '=', user.id), ('user_ids', 'in', user.id),
            '&amp;', ('user_id', '=', False), ('user_ids', '=', False),]</field>
        <field name="groups" eval="[(4, ref('base.group_user'))]" />
        <field name="perm_unlink" eval="False" />
    </record>
    <record id="base.ir_filters_employee_rule" model="ir.rule">
        <field name="active" eval="False" />
    </record>
</odoo>
